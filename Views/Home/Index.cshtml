@model List<TADTitleConversion.Models.AgingTable>
@{
    ViewBag.Title = "Home Page";

    int status = 0;
    foreach (var cis in ViewBag.Birlocation)
    {
        status = cis.status;
    }

    int clientcount = TADTitleConversion.Controllers.HomeController.ClientCount();
    int turnovercount = TADTitleConversion.Controllers.HomeController.TurnoverCount();
    int pulloutcount = TADTitleConversion.Controllers.HomeController.PullOutCount();
    int cancelledcount = TADTitleConversion.Controllers.HomeController.CancelledCount();

    int prebircount = TADTitleConversion.Controllers.HomeController.PreBIRCount();
    int bircount = TADTitleConversion.Controllers.HomeController.BIRCount();
    int prerodcount = TADTitleConversion.Controllers.HomeController.PreRODCount();
    int rodcount = TADTitleConversion.Controllers.HomeController.RODCount();
    int preassessorcount = TADTitleConversion.Controllers.HomeController.PreAssessorCount();
    int assessorcount = TADTitleConversion.Controllers.HomeController.AssessorCount();
    int completedcount = TADTitleConversion.Controllers.HomeController.CompletedCount();

    int bankcount = TADTitleConversion.Controllers.HomeController.BankCount();
    int hdmfcount = TADTitleConversion.Controllers.HomeController.HDMFCount();
    int ihffcount = TADTitleConversion.Controllers.HomeController.IHFCount();
    int totalfinancing = bankcount + hdmfcount + ihffcount;

    //BREAKDOWN WITH BIR
    int BWBBoholcount = TADTitleConversion.Controllers.HomeController.BWBBohol();
    int BWBNorthcount = TADTitleConversion.Controllers.HomeController.BWBNorth();
    int BWBSouthcount = TADTitleConversion.Controllers.HomeController.BWBSouth();
    int BWBMandauecount = TADTitleConversion.Controllers.HomeController.BWBMandaue();
    int BWBTalisaycount = TADTitleConversion.Controllers.HomeController.BWBTalisay();
    int totolcount = BWBBoholcount + BWBNorthcount + BWBSouthcount + BWBMandauecount + BWBTalisaycount;

    //BREAKDOWN WITH ROD
    int BWRBoholcount = TADTitleConversion.Controllers.HomeController.BWRBohol();
    int BWRCebuCitycount = TADTitleConversion.Controllers.HomeController.BWRCebuCity();
    int BWRCebuProvincecount = TADTitleConversion.Controllers.HomeController.BWRCebuProvince();
    int BWRMandauecount = TADTitleConversion.Controllers.HomeController.BWRMandaue();
    int BWRLapulapucount = TADTitleConversion.Controllers.HomeController.BWRLapulapu();
    int BWRtotolcount = BWRBoholcount + BWRCebuCitycount + BWRCebuProvincecount + BWRMandauecount + BWRLapulapucount;

    //BREAKDOWN WITH ASSESSOR
    int BWAMandauecount = TADTitleConversion.Controllers.HomeController.BWAMandaue();
    int BWACebuCitycount = TADTitleConversion.Controllers.HomeController.BWACebuCity();
    int BWALapulapucount = TADTitleConversion.Controllers.HomeController.BWALapulapu();
    int BWABoholcount = TADTitleConversion.Controllers.HomeController.BWABohol();
    int BWACebuProvincecount = TADTitleConversion.Controllers.HomeController.BWACebuProvince();
    int BWAtotolcount = BWAMandauecount + BWACebuCitycount + BWALapulapucount + BWABoholcount + BWACebuProvincecount;


    var GetAccessType_usingUsername = TADTitleConversion.Controllers.MastersController.GetAccessType_usingUsername(User.Identity.Name);
}

@{
    if (GetAccessType_usingUsername != "4")//Admistrator
    {
        <body class="background-White" style="background-color:white">
            <div class="row col-xs-12">
                <div class="col-xs-3 infobox-container">
                    <div class="infobox infobox-green">
                        <div class="infobox-icon">
                            <i class="ace-icon fa fa-users"></i>
                        </div>
                        <div class="infobox-data">
                            <span class="infobox-data-number">@clientcount</span>
                            <div class="pull-center">
                                <a class="infobox-content" href="/Masters/DocumentStatus" style="color:black; font-size:larger">Total Accounts</a>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-xs-3 infobox-container">
                    <div class="infobox infobox-blue">
                        <div class="infobox-icon">
                            <i class="ace-icon fa fa-inbox"></i>
                        </div>
                        <div class="infobox-data">
                            <span class="infobox-data-number">@turnovercount</span>
                            <div class="pull-center">
                                <a class="infobox-content" href="/Masters/TurnOverViewHistory" style="color: black; font-size: larger">Turn-Over</a>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-xs-3 infobox-container">
                    <div class="infobox infobox-pink">
                        <div class="infobox-icon">
                            <i class="ace-icon fa fa-briefcase"></i>
                        </div>
                        <div class="infobox-data">
                            <span class="infobox-data-number">@pulloutcount</span>
                            <div class="pull-center">
