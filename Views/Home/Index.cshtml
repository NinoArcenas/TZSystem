                        <div class="infobox-icon">
                            <i class="ace-icon glyphicon glyphicon-remove"></i>
                        </div>
                        <div class="infobox-data">
                            <span class="infobox-data-number">@cancelledcount</span>
                            <div class="pull-center">
                                <a class="infobox-content" href="/Masters/Archive#profile4" style="color: black; font-size: larger">Cancelled</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="row col-xs-12">
                <div class="col-xs-12 widget-container-col">
                    <div class="widget-box widget-color-blue">
                        <div class="widget-header" style="background-color:burlywood">
                            <h5 class="widget-title bigger lighter">
                                Financing
                            </h5>
                        </div>
                        <table id="developer" class="table table-striped table-bordered table-hover">
                            @{
                                <tr>
                                    <th class="center" style="background-color: bisque; color: black">
                                        <div class="pull-center">
                                            <label style="color:black">BANK</label>
                                        </div>
                                    </th>
                                    <th class="center" style="background-color: bisque; color: black"><span class="label label-warning arrowed arrowed-right">@bankcount</span></th>
                                </tr>
                                <tr>
                                    <th class="center" style="background-color: bisque; color: black">
                                        <div class="pull-center">
                                            <label style="color:black">HDMF</label>
                                        </div>
                                    </th>
                                    <th class="center" style="background-color: bisque; color: black"><span class="label label-warning arrowed arrowed-right">@hdmfcount</span></th>
                                </tr>
                                <tr>
                                    <th class="center" style="background-color: bisque; color: black">
                                        <div class="pull-center">
                                            <label style="color:black">IHF</label>
                                        </div>
                                    </th>
                                    <th class="center" style="background-color: bisque; color: black"><span class="label label-warning arrowed arrowed-right">@ihffcount</span></th>
                                </tr>
                                <tr>
                                    <th class="center" style="background-color: bisque; color: black">
                                        <div class="pull-center">
                                            <label style="color:black">TOTAL</label>
                                        </div>
                                    </th>
                                    <th class="center" style="background-color: bisque; color: black"><span class="label label-warning arrowed arrowed-right">@totalfinancing</span></th>
                                </tr>
                            }
                        </table>
                    </div>
                </div>
            </div>

            <div class="row col-xs-12">
                <div class="col-xs-6">
                    <div class="widget-box widget-color-blue">
                        <div class="table-header">
                            Document Status
                        </div>
                        <table id="developer" class="table table-striped table-bordered table-hover">
                            @{
                                <tr>
                                    <th class="center" style="background-color: lightskyblue">
                                        <div class="pull-center">
                                            <a class="center" href="/Masters/DocumentStatus" style="color:black">PRE-BIR</a>
                                        </div>
                                    </th>
                                    <th class="center" style="background-color: lightskyblue; color: black"><span class="label label-primary arrowed arrowed-right">@prebircount</span></th>
                                </tr>
                                <tr>
                                    <th class="center" style="background-color: lightskyblue">
                                        <div class="pull-center">
                                            <a class="center" href="/Masters/DocumentStatus" style="color:black">BIR</a>
                                        </div>
                                    </th>
                                    <th class="center" style="background-color: lightskyblue; color: black"><span class="label label-primary arrowed arrowed-right">@bircount</span></th>
                                </tr>
                                <tr>
                                    <th class="center" style="background-color: lightskyblue">
                                        <div class="pull-center">
                                            <a class="center" href="/Masters/DocumentStatus" style="color:black">PRE-ROD</a>
                                        </div>
                                    </th>
                                    <th class="center" style="background-color: lightskyblue; color:black"><span class="label label-primary arrowed arrowed-right">@prerodcount</span></th>
                                </tr>
                                <tr>
                                    <th class="center" style="background-color: lightskyblue">
                                        <div class="pull-center">
                                            <a class="center" href="/Masters/DocumentStatus" style="color:black">ROD</a>
                                        </div>
                                    </th>
                                    <th class="center" style="background-color: lightskyblue; color:black"><span class="label label-primary arrowed arrowed-right">@rodcount</span></th>
                                </tr>
                                <tr>
                                    <th class="center" style="background-color: lightskyblue">
                                        <div class="pull-center">
                                            <a class="center" href="/Masters/DocumentStatus" style="color:black">PRE-ASSESSOR</a>
                                        </div>
                                    </th>
                                    <th class="center" style="background-color: lightskyblue; color: black"><span class="label label-primary arrowed arrowed-right">@preassessorcount</span></th>
                                </tr>
                                <tr>
                                    <th class="center" style="background-color: lightskyblue">
                                        <div class="pull-center">
                                            <a class="center" href="/Masters/DocumentStatus" style="color:black">ASSESSOR</a>
                                        </div>
                                    </th>
                                    <th class="center" style="background-color: lightskyblue; color: black"><span class="label label-primary arrowed arrowed-right">@assessorcount</span></th>
                                </tr>
                                <tr>
                                    <th class="center" style="background-color: lightskyblue">
                                        <div class="pull-center">
                                            <a class="center" href="/Masters/DocumentStatus" style="color:black">COMPLETED</a>
                                        </div>
                                    </th>
                                    <th class="center" style="background-color: lightskyblue; color: black"><span class="label label-primary arrowed arrowed-right">@completedcount</span></th>
                                </tr>
                            }
                        </table>
                    </div>
                </div>
                <div class="col-xs-6">
                    <div class="widget-box widget-color-blue">
                        <div class="table-header" style="background-color:lightslategrey">
                            Aging Analysis
                        </div>
                        <table id="developer" class="table table-striped table-bordered table-hover">
                            @{
                                int totaltodayscount = 0;
                                foreach (var Days in ViewBag.TotalDaysCount)
                                {
                                    totaltodayscount = Days.ninetydays + Days.oneeighty + Days.treesixfive + Days.sevintreeziro + Days.sevintreeone;
                                    <tr>
                                        <th class="center" style="background-color: lightgrey; color: black">
                                            <div class="pull-center">
                                                <a style="color:black">1-90 days</a>
                                            </div>
                                        </th>
                                        <th class="center" style="background-color: lightgrey; color: black"><span class="label label-grey arrowed arrowed-right">@Days.ninetydays</span></th>
                                    </tr>
                                    <tr>
                                        <th class="center" style="background-color: lightgrey; color: black">
                                            <div class="pull-center">
                                                <a style="color:black">91- 180 days</a>
                                            </div>
                                        </th>
                                        <th class="center" style="background-color: lightgrey; color: black"><span class="label label-grey arrowed arrowed-right">@Days.oneeighty</span></th>
                                    </tr>
                                    <tr>
                                        <th class="center" style="background-color: lightgrey; color: black">
                                            <div class="pull-center">
                                                <a style="color:black">181-365 days</a>
                                            </div>
                                        </th>
                                        <th class="center" style="background-color: lightgrey; color: black"><span class="label label-grey arrowed arrowed-right">@Days.treesixfive</span></th>
                                    </tr>
                                    <tr>
                                        <th class="center" style="background-color: lightgrey; color: black">
                                            <div class="pull-center">
                                                <a style="color:black">366-730 days</a>
                                            </div>
                                        </th>
                                        <th class="center" style="background-color: lightgrey; color: black"><span class="label label-grey arrowed arrowed-right">@Days.sevintreeziro</span></th>
                                    </tr>
                                    <tr>
                                        <th class="center" style="background-color: lightgrey; color: black">
                                            <div class="pull-center">
                                                <a style="color:black">> 731 days</a>
                                            </div>
                                        </th>
                                        <th class="center" style="background-color: lightgrey; color: black"><span class="label label-grey arrowed arrowed-right">@Days.sevintreeone</span></th>
                                    </tr>
                                }
                                <tr>
                                    <th class="center" style="background-color: lightgrey; color: black">
                                        <div class="pull-center">
                                            <a style="color:black">TOTAL</a>
                                        </div>
                                    </th>
                                    <th class="center" style="background-color: lightgrey; color: black"><span class="label label-grey arrowed arrowed-right">@totaltodayscount</span></th>
                                </tr>
                            }
                        </table>
                    </div>
                </div>
            </div>

            <div class="row col-xs-12">
                <div class="col-xs-6">
                    <div class="widget-box widget-color-blue">
                        <div class="table-header">
                            BREAKDOWN WITH PRE-BIR
                        </div>
                        <table id="developer" class="table table-striped table-bordered table-hover">
                            @{
                                //BOHOL
                                int counterboholPreBir = 0;
                                int counterBOHOLBANKPreBir = 0;
                                int counterBOHOLIHFPreBir = 0;
                                int counterBOHOLHDMFPreBir = 0;
                                //MANDAUE
                                int counterMANDAUEPreBir = 0;
                                int counterMANDAUEBANKPreBir = 0;
                                int counterMANDAUEIHFPreBir = 0;
                                int counterMANDAUEHDMFPreBir = 0;
                                //NORTH
                                int counterNORTHPreBir = 0;
                                int counterNORTHBANKPreBir = 0;
                                int counterNORTHIHFPreBir = 0;
                                int counterNORTHDMFPreBir = 0;
                                //SOUTH
                                int counterSOUTHPreBir = 0;
                                int counterSOUTHBANKPreBir = 0;
                                int counterSOUTHIHFPreBir = 0;
                                int counterSOUTHHDMFPreBir = 0;
                                //TALISAY
                                int counterTALISAYPreBIR = 0;
                                int counterTALISAYBANKPreBIR = 0;
                                int counterTALISAYIHFPreBIR = 0;
                                int counterTALISAYHDMFPreBIR = 0;
                                //DUMAGUETE
                                int counteDUMAGUETEcountPreBIR = 0;
                                int counterDUMAGUETEBANKPreBIR = 0;
                                int counterDUMAGUETEIHFPreBIR = 0;
                                int counterDUMAGUETEHDMFPreBIR = 0;

                                int totalbirPreBIR = 0;
                                int totalbankPreBIR = 0;
                                int totalihfPreBIR = 0;
                                int totalhdmfPreBIR = 0;
                                foreach (var BIR in ViewBag.Account)
                                {
                                    //BOHOL
                                    if (BIR.location == "BOHOL" && BIR.status == 1)
                                    {
                                        counterboholPreBir++;
                                    }
                                    if (BIR.location == "BOHOL" && BIR.financing == "BANK" && BIR.status == 1)
                                    {
                                        counterBOHOLBANKPreBir++;
                                    }
                                    if (BIR.location == "BOHOL" && BIR.financing == "IHF" && BIR.status == 1)
                                    {
                                        counterBOHOLIHFPreBir++;
                                    }
                                    if (BIR.location == "BOHOL" && BIR.financing == "HDMF" && BIR.status == 1)
                                    {
                                        counterBOHOLHDMFPreBir++;
                                    }
                                    //MANDAUE
                                    else if (BIR.location == "MANDAUE" && BIR.status == 1)
                                    {
                                        counterMANDAUEPreBir++;
                                    }
                                    if (BIR.location == "MANDAUE" && BIR.financing == "BANK" && BIR.status == 1)
                                    {
                                        counterMANDAUEBANKPreBir++;
                                    }
                                    if (BIR.location == "MANDAUE" && BIR.financing == "IHF" && BIR.status == 1)
                                    {
                                        counterMANDAUEIHFPreBir++;
                                    }
                                    if (BIR.location == "MANDAUE" && BIR.financing == "HDMF" && BIR.status == 1)
                                    {
                                        counterMANDAUEHDMFPreBir++;
                                    }
                                    //NORTH
                                    else if (BIR.location == "NORTH" && BIR.status == 1)
                                    {
                                        counterNORTHPreBir++;
                                    }
                                    if (BIR.location == "NORTH" && BIR.financing == "BANK" && BIR.status == 1)
                                    {
                                        counterNORTHBANKPreBir++;
                                    }
                                    if (BIR.location == "NORTH" && BIR.financing == "IHF" && BIR.status == 1)
                                    {
                                        counterNORTHIHFPreBir++;
                                    }
                                    if (BIR.location == "NORTH" && BIR.financing == "HDMF" && BIR.status == 1)
                                    {
                                        counterNORTHDMFPreBir++;
                                    }
                                    //SOUTH
                                    else if (BIR.location == "SOUTH" && BIR.status == 1)
                                    {
                                        counterSOUTHPreBir++;
                                    }
                                    if (BIR.location == "SOUTH" && BIR.financing == "BANK" && BIR.status == 1)
                                    {
                                        counterSOUTHBANKPreBir++;
                                    }
                                    if (BIR.location == "SOUTH" && BIR.financing == "IHF" && BIR.status == 1)
                                    {
                                        counterSOUTHIHFPreBir++;
                                    }
                                    if (BIR.location == "SOUTH" && BIR.financing == "HDMF" && BIR.status == 1)
                                    {
                                        counterSOUTHHDMFPreBir++;
                                    }

                                    //TALISAY
                                    else if (BIR.location == "TALISAY" && BIR.status == 1)
                                    {
                                        counterTALISAYPreBIR++;
                                    }
                                    if (BIR.location == "TALISAY" && BIR.financing == "BANK" && BIR.status == 1)
                                    {

                                        counterTALISAYBANKPreBIR++;
                                    }
                                    if (BIR.location == "TALISAY" && BIR.financing == "IHF" && BIR.status == 1)
                                    {
                                        counterTALISAYIHFPreBIR++;
                                    }
                                    if (BIR.location == "TALISAY" && BIR.financing == "HDMF" && BIR.status == 1)
                                    {
                                        counterTALISAYHDMFPreBIR++;
                                    }
                                    //DUMAGUETE
                                    else if (BIR.location == "DUMAGUETE" && BIR.status == 1)
                                    {
                                        counteDUMAGUETEcountPreBIR++;
                                    }
                                    if (BIR.location == "DUMAGUETE" && BIR.financing == "BANK" && BIR.status == 1)
                                    {
                                        counterDUMAGUETEBANKPreBIR++;
                                    }
                                    if (BIR.location == "DUMAGUETE" && BIR.financing == "IHF" && BIR.status == 1)
                                    {
                                        counterDUMAGUETEIHFPreBIR++;
                                    }
                                    if (BIR.location == "DUMAGUETE" && BIR.financing == "HDMF" && BIR.status == 1)
                                    {
                                        counterDUMAGUETEHDMFPreBIR++;
                                    }
                                }
                                <tr>
                                    <th class="center" style="background-color: yellowgreen"><b>LOCATION</b></th>
                                    <th class="center" style="background-color: yellowgreen">TOTAL</th>
                                    <th class="center" style="background-color: yellowgreen">BANK</th>
                                    <th class="center" style="background-color: yellowgreen">IHF</th>
                                    <th class="center" style="background-color: yellowgreen">HDMF</th>
                                </tr>
                                <tr>
                                    @foreach (var BIRLocation in ViewBag.Birlocation)
                                    {
                                        totalbirPreBIR = counterboholPreBir + counterMANDAUEPreBir + counterNORTHPreBir + counterSOUTHPreBir + counterTALISAYPreBIR + counteDUMAGUETEcountPreBIR;
                                        totalbankPreBIR = counterBOHOLBANKPreBir + counterMANDAUEBANKPreBir + counterNORTHBANKPreBir + counterSOUTHBANKPreBir + counterTALISAYBANKPreBIR + counterDUMAGUETEBANKPreBIR;
                                        totalihfPreBIR = counterBOHOLIHFPreBir + counterMANDAUEIHFPreBir + counterNORTHIHFPreBir + counterSOUTHIHFPreBir + counterTALISAYIHFPreBIR + counterDUMAGUETEIHFPreBIR;
                                        totalhdmfPreBIR = counterBOHOLHDMFPreBir + counterMANDAUEHDMFPreBir + counterNORTHDMFPreBir + counterSOUTHHDMFPreBir + counterTALISAYHDMFPreBIR + counterDUMAGUETEHDMFPreBIR;
                                        if (BIRLocation.location == "BOHOL")
                                        {
                                            <th class="center" style="background-color: lightgreen">Bohol</th>
                                            <th class="center" style="background-color: lightgreen">@counterboholPreBir</th>
                                            <th class="center" style="background-color: lightgreen">@counterBOHOLBANKPreBir</th>
                                            <th class="center" style="background-color: lightgreen">@counterBOHOLIHFPreBir</th>
                                            <th class="center" style="background-color: lightgreen">@counterBOHOLHDMFPreBir</th>
                                        }
                                    }
                                </tr>
                                <tr>
                                    @{
                                        foreach (var BIRLocation in ViewBag.Birlocation)
                                        {
                                            if (BIRLocation.location == "MANDAUE")
                                            {
                                                <th class="center" style="background-color: lightgreen">Mandaue</th>
                                                <th class="center" style="background-color: lightgreen">@counterMANDAUEPreBir</th>
                                                <th class="center" style="background-color: lightgreen">@counterMANDAUEBANKPreBir</th>
                                                <th class="center" style="background-color: lightgreen">@counterMANDAUEIHFPreBir</th>
                                                <th class="center" style="background-color: lightgreen">@counterMANDAUEHDMFPreBir</th>
                                            }
                                        }
                                    }
                                </tr>
                                <tr>
                                    @foreach (var BIRLocation in ViewBag.Birlocation)
                                    {
                                        if (BIRLocation.location == "TALISAY")
                                        {
                                            <th class="center" style="background-color: lightgreen">Talisay</th>
                                            <th class="center" style="background-color: lightgreen">@counterTALISAYPreBIR</th>
                                            <th class="center" style="background-color: lightgreen">@counterTALISAYBANKPreBIR</th>
                                            <th class="center" style="background-color: lightgreen">@counterTALISAYIHFPreBIR</th>
                                            <th class="center" style="background-color: lightgreen">@counterTALISAYHDMFPreBIR</th>
                                        }
                                    }
                                </tr>
                                <tr>
                                    @foreach (var BIRLocation in ViewBag.Birlocation)
                                    {
                                        if (BIRLocation.location == "SOUTH")
                                        {
                                            <th class="center" style="background-color: lightgreen">South</th>
                                            <th class="center" style="background-color: lightgreen">@counterSOUTHPreBir</th>
                                            <th class="center" style="background-color: lightgreen">@counterSOUTHBANKPreBir</th>
                                            <th class="center" style="background-color: lightgreen">@counterSOUTHIHFPreBir</th>
                                            <th class="center" style="background-color: lightgreen">@counterSOUTHHDMFPreBir</th>
                                        }
                                    }
                                </tr>
                                <tr>
                                    @foreach (var BIRLocation in ViewBag.Birlocation)
                                    {
                                        if (BIRLocation.location == "NORTH")
                                        {
                                            <th class="center" style="background-color: lightgreen">North</th>
                                            <th class="center" style="background-color: lightgreen">@counterNORTHPreBir</th>
                                            <th class="center" style="background-color: lightgreen">@counterNORTHBANKPreBir</th>
                                            <th class="center" style="background-color: lightgreen">@counterNORTHIHFPreBir</th>
                                            <th class="center" style="background-color: lightgreen">@counterNORTHDMFPreBir</th>
                                        }
                                    }
                                </tr>
