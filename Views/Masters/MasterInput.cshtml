    @Scripts.Render("~/bundles/jqueryval")

    <script>

        $(document).ready(function () {

            //Save Developer
            $("#SaveDeveloper").click(function () {
                var developer = $("#Developer").val();
                if (developer != "") {
                    var url = "/Masters/Save_Docs"
                    $.post(url, { Dev: developer }, function (data) {
                        if (data == "savedeveloper") {
                            Swal.fire(
                                'Company Name successfully Saved!',
                                '',
                                'success'
                            ).then(function () {
                                location.reload();
                            });
                        }
                        else {
                            Swal.fire(
                                'This developer was already exist.',
                                '',
                                'error'
                            );
                        }
                    });
                }
                else {
                    Swal.fire({
                        icon: 'error',
                        title: 'Oops...',
                        text: 'Dont empty this field!',
                    }).then(function () {
                        if ($("#Developer").val() == "") {
                            document.getElementById("warningfielddeveloper").style.display = "block";
                        }
                    })
                }
            });
            $(".getdocs").click(function () {
                var id = this.id;
                var splits = id.split('*');
                var EditDeveloper = splits[0];
                var EditIDdeveloper = splits[1];
                $("#EditDeveloper").val(EditDeveloper);
                $("#EditIDdeveloper").val(EditIDdeveloper);
            });
            //Update Developer
