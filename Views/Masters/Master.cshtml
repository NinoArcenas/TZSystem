}

<body class="background-White" style="background-color:white">
    <div class="widget-body" style="background-color:white">
        <div class="row">
            <div class="col-xs-12">
                <div class="widget-box widget-color-green">
                    <div class="widget-header">
                        <div class="widget-title">
                            Account Process
                            @if (Accesstype == "1")
                            {
                                <div class="widget-toolbar">
                                    <div class="pull-center">
                                        <a class="btn btn-xs btn-primary btn-round no-hover getclientID" data-toggle="modal" data-target="#ModalEditClients" id="@id*@project*@company_name*@location*@rd_location*@ass_location*@financing*@idbir*@idrod*@idassessor*@fullpaymentdate">
                                            <i class="ace-icon fa fa-pencil-square-o bigger-110"></i>
                                            <span class="bigger-110">Edit</span>
                                        </a>
                                    </div>
                                </div>
                            }
                            else if (Accesstype == "2")
                            {
                                <div class="widget-toolbar">
                                    <div class="pull-center">
                                        <a class="btn btn-xs btn-primary btn-round no-hover getclientID" data-toggle="modal" data-target="#ModalEditClients" id="@id*@project*@company_name*@location*@rd_location*@ass_location*@financing*@idbir*@idrod*@idassessor*@fullpaymentdate">
                                            <i class="ace-icon fa fa-pencil-square-o bigger-110"></i>
                                            <span class="bigger-110">Edit</span>
                                        </a>
                                    </div>
                                </div>
                            }
                        </div>
                    </div>
                    <div class="widget-body">
                        <div class="widget-main">
                            <div class="col-xs-12 col-sm-4" style="display:none">
                                <label for="form-field-select-3">Status:</label>
                                <b for="form-field-select-2">@status.ToString()</b>
                                <input type="text" id="IDStatus" value="@status" class="width-100" />
                                <br />
                            </div>
                            <div class="col-xs-12 col-sm-3">
                                <b for="form-field-select-1">Account Name:</b>
                                <b class="blue" for="form-field-select-2">@account</b><br />
                                <b for="form-field-select-2">Project:</b>
                                <b class="blue" for="form-field-select-2">@project</b><br />
                                <b for="form-field-select-2">Notice of Full Payment Date:</b>
                                <b class="blue" for="form-field-select-2">@fullpaymentdate</b>
                                <br />
                            </div>
                            <div class="col-xs-12 col-sm-3">
                                <b for="form-field-select-2">Unit No:</b>
                                <b class="blue" for="form-field-select-2">@unit_no</b><br />
                                <b for="form-field-select-3">Company Name:</b>
                                <b class="blue" for="form-field-select-2">@company_name</b><br />
                            </div>
                            <div class="col-xs-12 col-sm-3">
                                <b for="form-field-select-3">BIR Location:</b>
                                <b class="blue" for="form-field-select-2">@location</b><br />
                                <b for="form-field-select-3">RD Location:</b>
                                <b class="blue" for="form-field-select-2">@rd_location</b><br />
                                <br />
                            </div>
                            <div class="col-xs-12 col-sm-3">
                                <b for="form-field-select-3">Assessor Location:</b>
                                <b class="blue" for="form-field-select-2">@ass_location</b><br />
                                <b for="form-field-select-3">Financing:</b>
                                <b class="blue" for="form-field-select-2" id="financingpagibig">@financing</b><br />
                                <br />
                            </div>
                            <div class="col-xs-12 col-sm-2" style="display:none">
                                <label for="form-field-select-3">Genecode:</label>
                                <b for="form-field-select-2">@generatedcode</b>
                                <br />
                            </div>
                            <div class="col-xs-12 col-sm-4" style="display:none">
                                <label for="form-field-select-3">ID:</label>
                                <b for="form-field-select-2">@id</b>
                                <input type="text" id="id" value="@id" class="width-100" />
                                <br />
                            </div>
                            <div class="col-xs-12 col-sm-4" style="display:none">
                                <label for="form-field-select-3">Aging ID:</label>
                                <b for="form-field-select-2">@idaging</b>
                                <input type="text" id="idagingreport" value="@idaging" class="width-100" />
                                <br />
                            </div>
                            <br />
                            <br />
                            <br />
                        </div>
                    </div>

                    <div class="modal fade" id="ModalEditClients" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
                        <div class="modal-dialog">
                            <div class="modal-content">
                                <div class="modal-header">
                                    <button type="button" style="text-align:left" class="close" data-dismiss="modal" aria-label="Close">
                                        <span aria-hidden="true">&times;</span>
                                    </button>
                                    <h5 class="modal-title" id="exampleModalLabel" style="color: black">Edit Account</h5>
                                </div>
                                <div class="modal-body">
                                    <form method="post">
                                        <div class="col-xs-12 col-sm-4" style="display:none">
                                            <label for="form-field-select-3">Status:</label>
                                            <b for="form-field-select-2">@status.ToString()</b>
                                            <input type="text" id="IDStatus" value="@status" class="width-100" />
                                            <br />
                                        </div>
                                        <div class="col-xs-12" style="background-color:white">
                                            <div class="profile-info-row">
                                                <div class="profile-info-name"><b>Project:</b></div>
                                                <div class="profile-info-value">
                                                    @Html.DropDownListFor(model => model.unit_no, new SelectList(ViewBag.proj, "Value", "Text"), "---Select Project Code---", new { @class = "chosen-select form-control input-mask-product", id = "EditProjectClient", value = "@project", style = "width: 140%;color:black" })
                                                </div>
                                            </div>
                                            <div class="profile-info-row">
                                                <div class="profile-info-name"><b>Company Name:</b></div>
                                                <div class="profile-info-value">
                                                    @Html.DropDownListFor(model => model.unit_no, new SelectList(ViewBag.development, "Value", "Text"), "---Select Company Name---", new { @class = "chosen-select form-control input-mask-product", id = "EditCompName", value = "@company_name", style = "width: 140%;color:black" })
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-xs-12" style="background-color:white">
                                            <div class="profile-info-row">
                                                <div class="profile-info-name"><b>BIR Location:</b></div>
                                                <div class="profile-info-value">
                                                    @Html.DropDownListFor(model => model.unit_no, new SelectList(ViewBag.location, "Value", "Text"), "---Select BIR Location---", new { @class = "chosen-select form-control input-mask-product", id = "EditBirLocation", value = "@location", style = "width: 236%;color:black" })
                                                </div>
                                            </div>
                                            <div class="profile-info-row">
                                                <div class="profile-info-name"><b>RD Location:</b></div>
                                                <div class="profile-info-value">
                                                    @Html.DropDownListFor(model => model.unit_no, new SelectList(ViewBag.rdlocation, "Value", "Text"), "---Select RD Location---", new { @class = "chosen-select form-control input-mask-product", id = "EditRdLocation", value = "@rd_location", style = "width: 236%;color:black" })
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-xs-12" style="background-color:white">
                                            <div class="profile-info-row">
