    var Fullpay = "";
    var Tcp = "";
    var Titleno = "";
    var TitlenoHideprebir = "";
    var TitlenoONEHideprebir = "";
    var TitlenoTWOHideprebir = "";
    var TitlenoTREEHideprebir = "";
    var TitlenoFOURHideprebir = "";
    var Taxdecno = "";
    var TaxdecnoHide = "";
    var TaxdecnoONEHide = "";
    var TaxdecnoTWOHide = "";
    var TaxdecnoTREEHide = "";
    var TaxdecnoFOURHide = "";
    var Doas = "";
    var Dst = "";
    var Notarizeddoas = "";
    var Transfertax = "";
    var Ewt = "";
    var Cts = "";
    var Receipts = "";
    var RemarksPreBIR = "";
    var GenCode = "";
    var Approveby = "";
    int idprebir = 0;
    foreach (var preBIR in ViewBag.prebir)
    {
        if (preBIR.generatedcode == generatedcode)
        {
            idprebir = preBIR.id;
            GenCode = preBIR.generatedcode;
            Block = preBIR.block;
            if (preBIR.unit_no != null)
            {
                Fullpay = Convert.ToString(String.Format("{0:yyyy-MM-dd}", Convert.ToDateTime(preBIR.unit_no)));
            }
            if (preBIR.doas != null)
            {
                Doas = Convert.ToString(String.Format("{0:yyyy-MM-dd}", Convert.ToDateTime(preBIR.doas)));
            }
            if (preBIR.notarized_doas != null)
            {
                Notarizeddoas = Convert.ToString(String.Format("{0:yyyy-MM-dd}", Convert.ToDateTime(preBIR.notarized_doas)));
            }
            if (preBIR.transfer_tax != null)
            {
                Transfertax = Convert.ToString(String.Format("{0:yyyy-MM-dd}", Convert.ToDateTime(preBIR.transfer_tax)));
            }
            if (preBIR.ewt != null)
            {
                Ewt = Convert.ToString(String.Format("{0:yyyy-MM-dd}", Convert.ToDateTime(preBIR.ewt)));
            }
            if (preBIR.cts != null)
            {
                Cts = Convert.ToString(String.Format("{0:yyyy-MM-dd}", Convert.ToDateTime(preBIR.cts)));
            }
            if (preBIR.dst != null)
            {
                Dst = Convert.ToString(String.Format("{0:yyyy-MM-dd}", Convert.ToDateTime(preBIR.dst)));
            }
            if (preBIR.receipts != null)
            {
                Receipts = Convert.ToString(String.Format("{0:yyyy-MM-dd}", Convert.ToDateTime(preBIR.receipts)));
            }
            Tcp = preBIR.tcp;
            Titleno = preBIR.title_no;
            TitlenoHideprebir = preBIR.title_no_hide;
            TitlenoONEHideprebir = preBIR.title_no_hide_one;
            TitlenoTWOHideprebir = preBIR.title_no_hide_two;
            TitlenoTREEHideprebir = preBIR.title_no_hide_tree;
            TitlenoFOURHideprebir = preBIR.title_no_hide_four;
