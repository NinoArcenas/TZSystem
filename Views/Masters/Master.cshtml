            {
                Titleavailabilitydate = Convert.ToString(String.Format("{0:yyyy-MM-dd}", Convert.ToDateTime(preROD.title_availability_date)));
            }
            if (preROD.date_completed != null)
            {
                Datecompleted = Convert.ToString(String.Format("{0:yyyy-MM-dd}", Convert.ToDateTime(preROD.date_completed)));
            }
            Dateaddedprerod = preROD.dateadded.ToString();
            if (preROD.ecar == 1)
            {
                strecar = "checked";
            }
            if (preROD.doas == 1)
            {
                strdoas = "checked";
            }
            if (preROD.dor == 1)
            {
                strdor = "checked";
            }
            if (preROD.sec_cert == 1)
            {
                strsec_cert = "checked";
            }
            if (preROD.td_ctc == 1)
            {
                strtd_ctc = "checked";
            }
            if (preROD.spa == 1)
            {
                strspa = "checked";
            }
            if (preROD.t_tax == 1)
            {
                strt_tax = "checked";
            }
            if (preROD.lma == 1)
            {
                strlma = "checked";
            }
            if (preROD.title == 1)
            {
                strtitle = "checked";
            }
            if (preROD.tc == 1)
            {
                strtc = "checked";
            }
            if (preROD.bir == 1)
            {
                strbir = "checked";
            }
            if (preROD.sec_cert_bylaws_sec_reg == 1)
            {
                strsec_cert_bylaws_sec_reg = "checked";
            }
            RemarkspreROD = preROD.remarks;
        }
    }


    //ROD
    var GenCodeROD = "";
    var Datesubmitted = "";
    var Datereleased = "";
    var Mortgagesub = "";
    var Mortgagerel = "";
    var Epebno = "";
    var TitlenoROD = "";
    var Epednohide = "";
    var EpednohideONE = "";
    var EpednohideTWO = "";
    var EpednohideTREE = "";
    var EpednohideFOUR = "";
    var Titlenohide = "";
    var TitlenohideONE = "";
    var TitlenohideTWO = "";
    var TitlenohideTREE = "";
    var TitlenohideFOUR = "";
    var RemarksROD = "";
    var Dateaddedrod = "";
    int idrod = 0;
    foreach (var ROD in ViewBag.rod)
    {
        if (ROD.generatedcode == generatedcode)
        {
            idrod = ROD.id;
            GenCodeROD = ROD.generatedcode;
            if (ROD.date_submitted != null)
            {
                Datesubmitted = Convert.ToString(String.Format("{0:yyyy-MM-dd}", Convert.ToDateTime(ROD.date_submitted)));
            }
            if (ROD.date_released != null)
            {
                Datereleased = Convert.ToString(String.Format("{0:yyyy-MM-dd}", Convert.ToDateTime(ROD.date_released)));
            }
            if (ROD.mortgage_sub != null)
            {
                Mortgagesub = Convert.ToString(String.Format("{0:yyyy-MM-dd}", Convert.ToDateTime(ROD.mortgage_sub)));
            }
            if (ROD.mortgage_rel != null)
            {
                Mortgagerel = Convert.ToString(String.Format("{0:yyyy-MM-dd}", Convert.ToDateTime(ROD.mortgage_rel)));
            }
            Epebno = ROD.epeb_no;
            TitlenoROD = ROD.title_no;
            Epednohide = ROD.eped_no_hide;
            EpednohideONE = ROD.eped_no_hide_one;
            EpednohideTWO = ROD.eped_no_hide_two;
            EpednohideTREE = ROD.eped_no_hide_tree;
            EpednohideFOUR = ROD.eped_no_hide_four;
            Titlenohide = ROD.title_no_hide;
            TitlenohideONE = ROD.title_no_hide_one;
            TitlenohideTWO = ROD.title_no_hide_two;
            TitlenohideTREE = ROD.title_no_hide_tree;
            TitlenohideFOUR = ROD.title_no_hide_four;
            RemarksROD = ROD.remarks;
            Dateaddedrod = ROD.dateadded.ToString();
        }
    }

    //Pre-Assessor
    var GenCodePreAsse = "";
    var Datecompletedpreass = "";
    var RemarksPreAss = "";
    int idpreassessor = 0;
    var strEcarPreAss = "";
    var strTaxdecPreAss = "";
    var strTaxclearance = "";
    var strDoasPreAss = "";
    var strTitlePreAss = "";
    var strTransfertaxPreAss = "";
    var strSwornstatement = "";
    var strSeccertPreAss = "";
    var strpicture = "";
    var strmanagement_cer = "";
    var strcover_page = "";
    var strOthers = "";
    foreach (var preAss in ViewBag.preass)
    {
        if (preAss.generatedcode == generatedcode)
        {
            idpreassessor = preAss.id;
            GenCodePreAsse = preAss.generatedcode;
            Datecompletedpreass = Convert.ToString(String.Format("{0:yyyy-MM-dd}", Convert.ToDateTime(preAss.date_completed)));
            if (preAss.ecar == 1)
            {
                strEcarPreAss = "checked";
            }
            if (preAss.tax_dec == 1)
            {
                strTaxdecPreAss = "checked";
            }
            if (preAss.tax_clearance == 1)
            {
                strTaxclearance = "checked";
            }
            if (preAss.doas == 1)
            {
                strDoasPreAss = "checked";
            }
            if (preAss.title == 1)
            {
                strTitlePreAss = "checked";
            }
            if (preAss.transfer_tax == 1)
            {
                strTransfertaxPreAss = "checked";
            }
            if (preAss.sworn_statement == 1)
