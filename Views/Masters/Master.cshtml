@model TADTitleConversion.Models.Client
@{
    ViewBag.Title = "Master";

    var Accesstype = TADTitleConversion.Controllers.MastersController.Accesstype(User.Identity.Name);

    string account = "";
    string project = "";
    string company_name = "";
    string location = "";
    string rd_location = "";
    string ass_location = "";
    string financing = "";
    var fullpaymentdate = "";
    string unit_no = "";
    string generatedcode = "";
    int status = 0;
    int id = 0;
    foreach (var cis in ViewBag.client)
    {
        id = cis.id;
        account = cis.account;
        project = cis.project;
        company_name = cis.company_name;
        status = cis.status;
        location = cis.location;
        rd_location = cis.rd_location;
        ass_location = cis.ass_location;
        financing = cis.financing;

        if (cis.notice_of_fullpayment_date != null)
        {
            fullpaymentdate = Convert.ToString(String.Format("{0:MM-dd-yyyy}", Convert.ToDateTime(cis.notice_of_fullpayment_date)));
        }
        unit_no = cis.unit_no;
        generatedcode = cis.generatedcode;
    }

    //Aging Report
    var accountname = "";
    var genered = "";
    int idaging = 0;
    foreach (var AgingReport in ViewBag.AgingReport)
    {
        if (AgingReport.generatedcode == generatedcode)
        {
            idaging = AgingReport.id;
            accountname = AgingReport.account;
            genered = AgingReport.generatedcode;
        }
    }


    var strstatus1 = "";
    var strstatus2 = "";
    var strstatus3 = "";
    var strstatus4 = "";
    var strstatus5 = "";
    var strstatus6 = "";
    var strstatus7 = "";
    if (status == 1)
    {
        strstatus1 = "active";
    }
    else if (status == 2)
    {
        strstatus2 = "active";
    }
    else if (status == 3)
    {
        strstatus3 = "active";
    }
    else if (status == 4)
    {
        strstatus4 = "active";
    }
    else if (status == 5)
    {
        strstatus5 = "active";
    }
    else if (status == 6)
    {
        strstatus6 = "active";
    }
    else if (status == 7)
    {
        strstatus7 = "active";
    }
    else
    {
        strstatus1 = "";
        strstatus2 = "";
        strstatus3 = "";
        strstatus4 = "";
        strstatus5 = "";
        strstatus6 = "";
        strstatus7 = "";
    }

    //Pre BIR
