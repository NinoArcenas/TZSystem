    var Released = "";
    var Ecarno = "";

    var birloc = "";
    var rodloc = "";
    var assloc = "";

    var EcarnoHide = "";
    var EcarnoONEHide = "";
    var EcarnoTWOHide = "";
    var EcarnoTREEHide = "";
    var EcarnoFOURHide = "";
    var Ecarexpirrydate = "";
    var RemarksBIR = "";
    var GenCodeBIR = "";
    var Dateadded = "";
    int idbir = 0;
    foreach (var BIR in ViewBag.bir)
    {
        if (BIR.generatedcode == generatedcode)
        {
            idbir = BIR.id;
            GenCodeBIR = BIR.generatedcode;
            if (BIR.submission != null)
            {
                Submission = Convert.ToString(String.Format("{0:yyyy-MM-dd}", Convert.ToDateTime(BIR.submission)));
            }
            if (BIR.released != null)
            {
                Released = Convert.ToString(String.Format("{0:yyyy-MM-dd}", Convert.ToDateTime(BIR.released)));
            }
            if (BIR.ecar_expirry_date != null)
