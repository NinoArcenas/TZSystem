            }
            if (preAss.picture == 1)
            {
                strpicture = "checked";
            }
            if (preAss.management_cer == 1)
            {
                strmanagement_cer = "checked";
            }
            if (preAss.cover_page == 1)
            {
                strcover_page = "checked";
            }
            if (preAss.others == 1)
            {
                strOthers = "checked";
            }
            RemarksPreAss = preAss.remarks;
        }
