    var strdoas = "";
    var strdor = "";
    var strsec_cert = "";
    var strtd_ctc = "";
    var strspa = "";
    var strt_tax = "";
    var strlma = "";
    var strtitle = "";
    var strtc = "";
    var strbir = "";
    var strsec_cert_bylaws_sec_reg = "";
    ; foreach (var preROD in ViewBag.prerod)
    {
        if (preROD.generatedcode == generatedcode)
        {
            idprerod = preROD.id;
