    //Assessor
    var GenCodeAsse = "";
    var TaxdecnoAss = "";
    var Taxdexnoland = "";
    var TaxdexnolandTWO = "";
    var TaxdexnolandTREE = "";
    var TaxdexnolandFOUR = "";
    var Landunitno = "";
    var Taxdexnobuilding = "";
    var TaxdexnobuildingTWO = "";
    var TaxdexnobuildingTREE = "";
    var TaxdexnobuildingFOUR = "";
    var DatereleasedAss = "";
    var RemarksAss = "";
    int idassessor = 0;
    foreach (var Ass in ViewBag.ass)
    {
        if (Ass.generatedcode == generatedcode)
        {
            idassessor = Ass.id;
            GenCodeAsse = Ass.generatedcode;
            TaxdecnoAss = Ass.tax_dec_no;
            Taxdexnoland = Ass.tax_dex_no_land;
            TaxdexnolandTWO = Ass.tax_dex_no_land_two;
            TaxdexnolandTREE = Ass.tax_dex_no_land_tree;
