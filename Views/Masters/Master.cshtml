                Ecarexpirrydate = Convert.ToString(String.Format("{0:yyyy-MM-dd}", Convert.ToDateTime(BIR.ecar_expirry_date)));
            }
            birloc = BIR.bir_location;
            rodloc = BIR.rod_location;
            assloc = BIR.assessor_location;
            Ecarno = BIR.ecar_no;
            EcarnoHide = BIR.ecar_no_hide;
            EcarnoONEHide = BIR.ecar_no_hide_one;
            EcarnoTWOHide = BIR.ecar_no_hide_two;
            EcarnoTREEHide = BIR.ecar_no_hide_tree;
            EcarnoFOURHide = BIR.ecar_no_hide_four;
            RemarksBIR = BIR.remarks;
            Dateadded = BIR.dateadded.ToString();
        }
    }

    //Pre ROD
    //var Receiveddate = "";
    var Titlerequestate = "";
    var Titleavailabilitydate = "";
    var Datecompleted = "";
    var RemarkspreROD = "";
    var Dateaddedprerod = "";
